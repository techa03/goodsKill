	<version>${goodsKill.version}</version>
	<packaging>jar</packaging>
	<parent>
		<groupId>com.goodskill</groupId>
		<artifactId>goodsKill-spring-boot-provider</artifactId>
		<version>3.0.0-SNAPSHOT</version>
